#include <stdio.h>

void update(int *a,int *b) {
    int x,y;
    x=(*a)+(*b);
     if((*a)>(*b))
     {
        y =(*a)-(*b);

     }   
     else
      if((*a)<=(*b))
      {
         y=(*b)-(*a);
 
      }
 
    (*a)=x;
    (*b)=y;
  
}

int main() {
    int a, b;
  
    
    scanf("%d %d", &a, &b);
    update(&a,&b);
    printf("%d\n%d", a, b);

    return 0;
}
