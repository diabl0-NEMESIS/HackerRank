#include<stdio.h>

int SimpleArraySum(int a[],int size)
{  int s=0;
 for(int i=0;i<size;i++)
  {
   
 s =s +a[i];
  }
 return s;
}

int main()
{   int a[100000],s,sum=0;
     scanf("%d",&s);
     for(int i=0;i<s;i++)
     scanf("%d",&a[i]);
     
     sum=SimpleArraySum(a,s);
     printf("%d ",sum);
 return 0;
}
